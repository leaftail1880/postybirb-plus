    "yarn": "^1.22.4",
    "@jest-runner/electron": "^3.0.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/cheerio": "^0.22.30",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^8.1.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.175",
    "@types/lowdb": "^1.0.11",
